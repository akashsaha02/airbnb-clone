{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "cache": {
        "edge": {
            "maxAge": 60,
            "staleWhileRevalidate": 30
        }
    }
}